  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "budo": "^9.4.7"
  },
	"browserify": {
    "transform": [
      "babelify"
    ]
  }
